cd $PBS_O_WORKDIR

source $HOME/.bashrc
export LD_LIBRARY_PATH="grackle/local/lib:$LD_LIBRARY_PATH"
time mpirun -np 32 ./ICs/cluster_cooling_150/GIZMO ICs/cluster_cooling_150/cluster_cooling_150.params.restart 2
