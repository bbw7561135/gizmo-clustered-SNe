#!/bin/bash
#
#PBS -l nodes=2:ppn=16
#PBS -l walltime=48:00:00
#
#PBS -q normal
#
#PBS -M egentry@ucsc.edu
#PBS -m e
#
#PBS -e ./ICs/cluster_cooling_150/
#PBS -o ./ICs/cluster_cooling_150/
#
#PBS -S /bin/bash
